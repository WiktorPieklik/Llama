{
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.search.rootFiles.exclude": ["**/parts/**/*.tex"],
  "latex-workshop.latex.outDir": "%DIR%/../out",
  "plantuml.diagramsRoot": "docs/2_opis/src/diagrams/src",
  "plantuml.exportOutDir": "docs/2_opis/src/diagrams/out",
  "plantuml.exportSubFolder": false,

  "[latex]": {
    "editor.wordWrap": "bounded"
  },
  "spellright.language": ["English (American)", "English (British)", "Polish"],
  "spellright.documentTypes": [
    "markdown",
    "latex"
  ],
  "python.pythonPath": "venv/bin/python3.7",
  "python.formatting.provider": "black"
}
